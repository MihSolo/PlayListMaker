<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ButtonMainStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/layout_height_main</item>
        <item name="android:layout_marginHorizontal">@dimen/mergin_16dp</item>
        <item name="android:layout_weight">@dimen/layout_weight</item>
        <item name="android:backgroundTint">@color/button_color</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/text_button</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="cornerRadius">@dimen/corner_radius_main_button</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/icon_padding</item>
        <item name="iconTint">@color/icon_color</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="ButtonBackToMain">
        <item name="android:layout_gravity">start</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:contentDescription">null</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/icon_color</item>
        <item name="icon">@drawable/arrow</item>
    </style>

    <style name="FrameLayoutSettings">
        <item name="android:layout_gravity">start</item>
        <item name="android:paddingLeft">@dimen/padding_16dp</item>
    </style>

    <style name="LinearLayoutBasicForAllDesctop">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="LinearLayoutViewOnAppDesktopTop">
        <item name="android:layout_gravity">top</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="TextViewTitelNearBackArrow">
        <item name="android:textSize">@dimen/textSize_universal_bookmark_titel</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:minHeight">@dimen/minHeight_text</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:paddingLeft">@dimen/paddingLeft_Lib_Srch</item>
    </style>

    <style name="TextViewSettingsMenyForAll">
        <item name="android:layout_gravity">start</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">@dimen/minHeight_text</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/textSize_16sp_set_act</item>

    </style>

    <style name="SettingsButtonViewSupport">
        <item name="android:layout_gravity">end</item>
        <item name="android:scaleType">center</item>
        <item name="drawableEndCompat">@drawable/support</item>

    </style>

    <style name="SettingsButtonViewShareMode">
        <item name="android:layout_gravity">end</item>
        <item name="android:scaleType">center</item>
        <item name="drawableEndCompat">@drawable/share</item>

    </style>

    <style name="SettingsButtonViewArrow">
        <item name="android:layout_gravity">end</item>
        <item name="android:scaleType">center</item>
        <item name="drawableEndCompat">@drawable/arrow_forward</item>
    </style>

    <style name="Switch_NightMode">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:scaleType">center</item>
        <item name="trackTint">#E6E8EB</item>
        <item name="thumbTint">#AEAFB4</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:checked">false</item>
    </style>

    <style name="LinearLayout_SearchDispley_Border_for_EditText">
        <item name="android:layout_gravity">top</item>
        <item name="android:paddingLeft">@dimen/padding_16dp</item>
        <item name="android:paddingRight">@dimen/padding_16dp</item>
        <item name="android:paddingTop">@dimen/padding_8dp</item>
        <item name="android:paddingBottom">@dimen/padding_8dp</item>
    </style>

    <style name="TextInputLayout_SearchDisplay">
        <item name="android:textColor">@color/searchIconTintColor</item>
        <item name="endIconTint">@color/searchIconTintColor</item>
        <item name="startIconTint">@color/searchIconTintColor</item>
        <item name="boxBackgroundColor">@color/searchBackgroundColor</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/TextInputLayout_Corners_SearchDisplay</item>
        <item name="boxCornerRadiusTopEnd">@dimen/TextInputLayout_Corners_SearchDisplay</item>
        <item name="boxCornerRadiusBottomStart">@dimen/TextInputLayout_Corners_SearchDisplay</item>
        <item name="boxCornerRadiusTopStart">@dimen/TextInputLayout_Corners_SearchDisplay</item>
        <item name="endIconMode">clear_text</item>
        <item name="startIconDrawable">@drawable/magnifying_glass</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="expandedHintEnabled">true</item>
        <item name="hintEnabled">false</item>
        <item name="boxStrokeColor">@color/white</item>
    </style>

    <style name="EditText_SearchDisplay">
        <item name="android:hint">@string/search</item>
        <item name="android:padding">1dp</item>
        <item name="android:drawablePadding">-17dp</item>
        <item name="android:maxLength">50</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
    </style>

    <style name="CardViewBasement_cardView">
        <item name="android:layout_height">@dimen/cardViewBasementHeight</item>
        <item name="cardElevation">@dimen/cardViewBasementElevation</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="CardViewImageView_trackCover">
        <item name="android:layout_width">@dimen/CardViewImageViewWidth</item>
        <item name="android:layout_height">@dimen/CardViewImageViewHeight</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginTop">@dimen/CardViewImageView_layout_marginTop</item>
        <item name="android:layout_marginLeft">@dimen/CardViewImageView_layout_marginLeft</item>
    </style>

    <style name="CardViewTextView_artistNameView">
        <item name="android:layout_height">@dimen/cardView_artistNameView_ViewHeight</item>
        <item name="android:layout_width">@dimen/cardView_artistNameView_ViewWidth</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginLeft">@dimen/cardView_artistNameView_layout_marginLeft
        </item>
        <item name="android:layout_marginTop">@dimen/cardView_artistNameView_layout_marginTop</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/cardView_artistNameView_textSize</item>
    </style>

    <style name="CardViewFrameLayout_frameLayout_border">
        <item name="android:layout_width">@dimen/cardView_frameLayout_border_ViewWidth</item>
        <item name="android:layout_height">@dimen/cardView_frameLayout_border_ViewHeight</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginTop">@dimen/cardView_frameLayout_border_layout_marginTop
        </item>
        <item name="android:layout_marginLeft">
            @dimen/cardView_frameLayout_border_layout_marginLeft
        </item>
        <item name="android:orientation">horizontal</item>

    </style>

    <style name="CardViewTextView_trackNameView">
        <item name="android:layout_height">@dimen/cardView_trackNameView_ViewHeight</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/textColor_CardViewTextView_trackNameView</item>
        <item name="android:textSize">@dimen/cardView_trackNameView_textSize</item>
        <item name="android:maxLength">30</item>
    </style>

    <style name="CardView_ImageView_dot">
        <item name="android:layout_height">@dimen/cardView_dot_ViewHeight</item>
        <item name="android:layout_width">@dimen/cardView_dot_ViewWidth</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/dot</item>
    </style>

    <style name="CardView_TextView_trackTimeView">
        <item name="android:layout_width">@dimen/cardView_trackTimeView_layoutWidth</item>
        <item name="android:layout_height">@dimen/cardView_trackNameView_ViewHeight</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/textColor_CardViewTextView_trackNameView</item>
        <item name="android:textSize">@dimen/cardView_trackNameView_textSize</item>
    </style>

    <style name="CardView_textView_SettingsButtonViewArrow">
        <item name="android:layout_height">@dimen/cardView_SettingsButtonViewArrow_ViewHeight</item>
        <item name="android:layout_width">@dimen/cardView_SettingsButtonViewArrow_ViewWidth</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:scaleType">center</item>
        <item name="drawableEndCompat">@drawable/light_mode_recycleview</item>
        <item name="android:layout_marginTop">
            @dimen/cardView_SettingsButtonViewArrow_layout_marginTop
        </item>
        <item name="android:layout_marginRight">
            @dimen/cardView_SettingsButtonViewArrow_layout_marginRight
        </item>
    </style>


    <style name="Placeholder">
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/placeholder_text_size</item>
        <item name="android:textColor">?attr/colorAccent</item>
    </style>

    <style name="ButtonRefresh" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/button_refresh_size</item>
        <item name="android:textColor">?attr/helperTextTextColor</item>
        <item name="android:letterSpacing">0</item>
        <item name="cornerRadius">@dimen/size_54</item>
        <item name="backgroundTint">?attr/colorAccent</item>
    </style>

    <style name="no_song_base_LinearLayout">
        <item name="android:layout_marginTop">102dp</item>
        <item name="android:orientation">vertical</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toBottomOf">@+id/SearchField</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="no_song_textView_image">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="layout_editor_absoluteX">187dp</item>
        <item name="layout_editor_absoluteY">359dp</item>
        <item name="drawableEndCompat">@drawable/no_song</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="no_song_textView_text">
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">19sp</item>
        <item name="layout_editor_absoluteX">142dp</item>
        <item name="layout_editor_absoluteY">481dp</item>
    </style>

    <style name="no_internet_base_LinearLayout">
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginTop">102dp</item>
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:visibility">gone</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toBottomOf">@+id/SearchField</item>
    </style>

    <style name="no_internet_textView_image">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:gravity">center</item>
        <item name="layout_editor_absoluteX">187dp</item>
        <item name="layout_editor_absoluteY">359dp</item>
        <item name="drawableEndCompat">@drawable/no_internet</item>
    </style>

    <style name="no_internet_textView_text">
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">19sp</item>
        <item name="layout_editor_absoluteX">142dp</item>
        <item name="layout_editor_absoluteY">481dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="no_internet_searchButton">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">14sp</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>